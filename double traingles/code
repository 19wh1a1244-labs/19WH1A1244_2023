#include <iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int p=1,k;
    for(int i=n;i>0;i--){
        k=p;
        for(int j=i;j>0;j--){
            cout<<k<<" ";
            k++;
        }
        p++;
        cout<<"\n";
    }
    p=5;
    for(int i=2;i<=5;i++){
        p--;
        k=p;
        for(int j=1;j<=i;j++){
            cout<<k<<" ";
            k++;
        }
        cout<<"\n";
    }

    return 0;
}
